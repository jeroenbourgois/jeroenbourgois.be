*{box-sizing:border-box}html{margin:0;padding:0;width:100%}body{background:#fff;color:#000;font-size:16px;line-height:1.4;padding:0;margin:0}h1{font-size:1.8em}h2{font-size:1.125em}h3{font-size:1.0125em}figure{padding:0;margin:0;max-width:50rem;background:#fff5be}figure a,figure img{width:100%;display:block}figure>figcaption{text-align:center;width:100%;font-family:courier;font-size:12px;display:block}figure>figcaption>p{padding:5px;margin-top:0}.read-more{display:block;margin-top:10px}main{width:100%;max-width:60em;padding:10px}aside{padding:20px}.site-header{background:#efefef;padding:5px 10px}.nav{display:flex;padding:0;margin:0}.nav ul{padding:0;margin:0;list-style:none}.nav li{list-style:none;display:inline}.nav a{margin-right:10px;display:inline-block;text-decoration:none}a{color:blue}a:visited{color:#191970}p{margin-bottom:1rem}.timestamp{font-size:small;font-family:courier}.site-footer{border-top:1px solid #ccc;display:flex;padding:10px;font-size:80%;align-items:center;justify-content:space-between;margin-top:1.5em}time{margin-bottom:5px;color:dimgrey;display:inline-block}.posttitle{margin-bottom:0}.posttitle+time{font-size:small;display:block;margin-bottom:1rem}@media only screen and (min-device-width:768px) and (max-device-width:1024px) and (-webkit-min-device-pixel-ratio:1) and (orientation:landscape){.maintext,.sidebar{font-size:90%}}pre,code{font-size:16px;word-wrap:break-word;font-family:monaco,monospace}pre{line-height:1.2;background:#efefef;padding:10px;width:100%;display:block;overflow-x:scroll}a{color:blue}h1 a,h2 a,h3 a,h4 a{text-decoration:none;color:inherit}.author{display:none}img{max-width:100%}pre.sourceCode span.st{color:#366354}pre.sourceCode span.kw{color:#397460}pre.sourceCode span.fu{color:#8f4e8b}pre.sourceCode span.ot{color:#2e659c}pre.sourceCode span.dt{color:#4f4371}pre.sourceCode span.co{color:#666}